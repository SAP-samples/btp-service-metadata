              "parameters": {
                "$schema": "http://json-schema.org/draft-06/schema#",
                "_controlsOrder": [
                  "resourceId",
                  "subResource",
                  "requestMessage",
                  "cosmosDb"
                ],
                "_show_form_view": true,
                "additionalProperties": false,
                "properties": {
                  "cosmosDb": {
                    "description": "Specifies CosmosDB-specific creation parameters.",
