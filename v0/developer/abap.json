      "schemas": {
        "service_instance": {
          "create": {
            "parameters": {
              "$schema": "http://json-schema.org/draft-07/schema",
              "additionalProperties": false,
              "type": "object",
              "_show_form_view": true,
              "_controlsOrder": [
                "admin_email",
                "description",
                "is_development_allowed",
                "sapsystemname",
                "size_of_runtime",
                "size_of_persistence"
              ],
              "properties": {
                "admin_email": {
                  "title": "Admin Email Address",
                  "description": "Enter the administrator's email address",
                  "type": "string",
                  "minLength": 6,
                  "maxLength": 256,
                  "pattern": "^[^<>()\\[\\]\\\\,;:\\s@\"`]+@([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}$"
                },
                "description": {
                  "title": "ABAP System Description",
                  "description": "Enter a description for the ABAP system",
                  "type": "string",
                  "maxLength": 256,
                  "pattern": "^[^'\\x00-\\x1f]*$"
                },
                "sapsystemname": {
                  "title": "ABAP System ID",
                  "description": "Enter a valid system ID (SID) for the ABAP system. The ID must consist of exactly three alphanumeric characters. Only uppercase letters are allowed. The first character must be a letter (not a digit). The ID does not have to be technically unique. The following IDs are reserved and cannot be used: ADD ALL AMD AND ANY ARE ASC AUX AVG BIT CDC COM CON DBA END EPS FOR GET GID IBM INT KEY LOG LPT MAP MAX MIN MON NIX NOT NUL OFF OLD OMS OUT PAD PRN RAW REF ROW SAP SET SGA SHG SID SQL SUM SYS TMP TOP UID USE USR VAR.",
                  "type": "string",
                  "default": "H01",
                  "minLength": 3,
                  "maxLength": 3,
                  "pattern": "^(?!ADD|ALL|AMD|AND|ANY|ARE|ASC|AUX|AVG|BIT|CDC|COM|CON|DBA|END|EPS|FOR|GET|GID|IBM|INT|KEY|LOG|LPT|MAP|MAX|MIN|MON|NIX|NOT|NUL|OFF|OLD|OMS|OUT|PAD|PRN|RAW|REF|ROW|SAP|SET|SGA|SHG|SID|SQL|SUM|SYS|TMP|TOP|UID|USE|USR|VAR)[A-Z][A-Z0-9]{2}$"
                },
                "is_development_allowed": {
                  "title": "Development System",
                  "description": "Specify, if development shall be allowed on this ABAP system",
                  "type": "boolean",
                  "default": true
                },
                "size_of_persistence": {
                  "title": "HANA Memory Size",
                  "description": "Enter the size of the HANA memory in blocks of 15 GB on AWS",
                  "type": "integer",
                  "default": 4,
                  "const": 4
                },
                "admin_user_name": {
                  "title": "Admin User Name",
                  "description": "Optionally enter the username of the administrator. The username must not begin with SAP_ or _, must only contain uppercase letters, digits, underscores, hyphens and periods, and must not be longer than 40 characters.",
                  "type": "string",
                  "minLength": 0,
                  "maxLength": 40,
                  "pattern": "^(?!SAP_|_)[\\.A-Z0-9_-]{0,40}$"
                },
                "login_attribute": {
                  "title": "Login Attribute",
                  "description": "Which attribute should be used for login.",
                  "type": "string",
                  "enum": [
                    "email",
                    "user_name"
                  ],
                  "default": "email"
                },
                "size_of_runtime": {
                  "title": "ABAP Runtime Size",
                  "description": "Enter the size of the ABAP runtime in blocks of 16 GB",
                  "type": "integer",
                  "default": 1,
                  "const": 1
                }
              },
              "required": [
                "admin_email",
                "size_of_persistence",
                "size_of_runtime"
              ]
            }
          },
          "update": {
            "parameters": {
              "$schema": "http://json-schema.org/draft-07/schema",
              "additionalProperties": false,
              "type": "object",
              "_show_form_view": true,
              "_controlsOrder": [
                "description",
                "size_of_runtime"
              ],
              "properties": {
                "description": {
                  "title": "ABAP System Description",
                  "description": "Enter a description for the ABAP system",
                  "type": "string",
                  "maxLength": 256,
                  "pattern": "^[^'\\x00-\\x1f]*$"
                },
                "size_of_runtime": {
                  "title": "ABAP Runtime Size",
                  "description": "Enter the size of the ABAP runtime in blocks of 16 GB",
                  "type": "integer",
                  "default": 1,
                  "const": 1
                }
              }
            }
          }
        }
      }
